.back{
  width: 100%;
  height: auto;
  min-height: 100vh;
  background-color: #f6f6f6;
}
.sousuo-input{
  width: 555rpx;
  height: 70rpx;
  background-color: #fff;
  border-radius: 35rpx;
  text-align: center;
  line-height: 70rpx;
  font-size: 25rpx;
  color: #efdf08;
  margin-left: 40rpx;
  margin-right: 35rpx;
  border: 1rpx solid #efdf08;
  float: left;
}
.sousuo-logo{
  width: 40rpx;
  height: 40rpx;
  display: block;
  float: left;
  margin-top: 15rpx;
}
.dangqian-city{
  width: 100%;
  height: 90rpx;
  background-color: #fff;
  padding-left: 50rpx;
  box-sizing: border-box;
  font-size: 30rpx;
  line-height: 90rpx;
  margin-top: 100rpx;
  margin-bottom: 10rpx;
}
.scroll-view{
  width: 100%;
  height: auto;
  background-color: #fff;
}
.city-text{
  width: 90%;
  height: 85rpx;
  padding: 0 10rpx;
  padding-top: 35rpx;
  padding-bottom: 20rpx;
  border-bottom: 1rpx solid #aaa;
  box-sizing: border-box;
  font-size: 30rpx;
  line-height: 30rpx;
  margin: 0 auto;
}
.city-more{
  width: 20rpx;
  height: 30rpx;
  display: block;
  float: right;
}



.none-display{
  display: none;
}
.display{
  display: block;
}