}
.xiahuaxian{
  height: 5rpx;
  background-color: #ffe018;
  margin-top: -5rpx;
}
.every-order{
  width: 100%;
  height: auto;
  padding: 10rpx 30rpx;
  box-sizing: border-box;
  background-color: #fff;
  overflow: hidden;
  border-radius: 10rpx;
  margin-bottom: 30rpx;
  box-shadow: 0rpx 5rpx 20rpx 5rpx #eee;
}
.order-img{
  width: 170rpx;
  height: 115rpx;
  border-radius: 10rpx;
  display: block;
  float: left;
  margin-right: 35rpx;
}
.order-name{
  width: 50%;
  height: auto;
  font-size: 30rpx;
  line-height: 30rpx;
  float: left;
  overflow: hidden;
  margin-top: 25rpx;
}
.xiaoliangqu{
  width: 50%;
  height: auto;
  font-size: 25rpx;
  line-height: 25rpx;
  margin-top: 20rpx;
