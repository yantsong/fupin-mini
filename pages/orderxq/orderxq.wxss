  float: left;
  border-radius: 50%;
  margin-right: 16rpx;
  /* background-color: red; */
}
.user-name{
  width: auto;
  font-size: 30rpx;
  line-height: 30rpx;
  float: left;
  color: #ffe018;
  font-weight: bold;
}
.user-phone{
  width: auto;
  height: auto;
  font-size: 26rpx;
  font-weight: bold;
  float: left;
  margin-left: 20rpx;
}
.user-dizhi{
  width: 80%;
  height: 30rpx;
  overflow: hidden;
  margin-top: 10rpx;
  float: left;
}
.user-dizhi-logo{
  width: 26rpx;
  height: 30rpx;
  display: block;
  float: left;
  margin-right: 20rpx;
  /* background-color: red; */
}
.user-dizhi-text{
  font-size: 30rpx;
  line-height: 30rpx;
  float: left;
}
.shezhi{
  width: 44rpx;
