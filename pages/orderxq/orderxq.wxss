.back{
  width: 100%;
  height: auto;
  min-height: 100vh;
  background-color: #f6f6f6;
}
.user-xinxi{
  width: 670rpx;
  height: 120rpx;
  border-radius: 20rpx;
  background-color: #f8faff;
  margin: 0 auto;
  padding: 25rpx 10rpx;
  box-sizing: border-box;
  box-shadow: 0 10rpx 20rpx #ddd;
}
.users-xinxi{
  width: 80%;
  float: left;
  height: 30rpx;
  overflow: hidden;
}
.user-logo{
