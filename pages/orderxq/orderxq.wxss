  line-height: 50rpx;
  font-size: 28rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.phone image{
  padding: 10rpx;
  width: 50rpx;
  height: 50rpx;
}
.dian-name{
  width: auto;
  height: auto;
