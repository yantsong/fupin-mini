.catgroy{min-height:100vh;background-color:#f5f5f5}.catgroy-tabs{padding:21rpx 60rpx;display:flex;align-items:center;justify-content:space-between;flex-wrap:wrap}.catgroy-tabs-item{width:30%;margin-bottom:20rpx;height:52rpx;line-height:52rpx;font-size:26rpx;align-items:center;background-color:#fff;color:#333333;box-shadow:-3rpx 5rpx 24rpx 0rpx rgba(113,113,113,0.23);border-radius:25rpx;text-align:center}.catgroy-tabs-item.active{background-color:#ffe018}.catgroy-list-item{background-color:#fff;display:flex;align-items:center;justify-content:flex-start;width:628rpx;padding:25rpx 0 32rpx;margin:0 auto 15rpx;text-align:left;box-shadow:1px 9px 11px 2px rgba(213,213,213,0.39);border-radius:25px}.catgroy-list-item-img{width:160rpx;height:160rpx;border-radius:20rpx;transform:translateX(-42rpx)}.catgroy-list-item .info-title{font-size:28rpx;line-height:52rpx;font-weight:normal;font-stretch:normal;letter-spacing:0rpx;color:#333333}.catgroy-list-item .info-sale{font-size:22rpx;font-weight:normal;font-stretch:normal;letter-spacing:0rpx;color:#999999;margin-bottom:34rpx}.catgroy-list-item .info-price{font-size:24rpx;color:#dd0000}.catgroy-list-item .info-price .num{font-size:32rpx;display:inline-block}.catgroy .info-sale{display:flex;align-items:center}.catgroy .info-sale-icon{width:20rpx;height:20rpx;margin-right:5rpx}.catgroy .nofoods{text-align:center;margin-top:30rpx;font-size:26rpx;color:#666}
