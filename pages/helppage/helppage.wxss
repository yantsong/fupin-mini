.helppage{box-sizing:border-box;min-height:100vh;background-color:#f5f5f5;padding:0 24rpx;margin-top:15rpx}.helppage .helppage-list{width:100%;box-sizing:border-box;padding:0 24rpx;margin-bottom:16rpx;background-color:#ffffff;box-shadow:2px 1px 18px 0px rgba(141,141,141,0.28);border-radius:10px}.helppage .helppage-list .list-title{font-size:32rpx;line-height:70rpx;font-family:AdobeHeitiStd-Regular;font-weight:normal;font-stretch:normal;letter-spacing:0px;color:#333333}.helppage .helppage-list .list-desc{font-size:28rpx;line-height:36rpx;font-weight:normal;font-stretch:normal;letter-spacing:0px;color:#666666}.helppage .helppage-list .list-imgs{display:flex;align-items:center;justify-content:space-between;margin-top:10rpx}.helppage .helppage-list .list-imgs-item{width:133rpx;height:133rpx;border-radius:10rpx}.helppage .helppage-list .list-time{padding:36rpx 0 20rpx;text-align:right;font-size:22rpx;font-weight:normal;font-stretch:normal;letter-spacing:0rpx;color:#999999}
